{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@layout/*": ["components/layout/*"],
      "@comman/*": ["components/comman/*"],
      "@assets/*": ["assets/*"],
      "@capacityBuilding/*": ["components/capacityBuilding/*"],
      "@circulars/*": ["components/circulars/*"],
      "@dashboard/*": ["components/dashboard/*"],
      "@inventory/*": ["components/inventory/*"],
      "@logistics/*": ["components/logistics/*"],
      "@maintenance/*": ["components/maintenance/*"],
      "@memo/*": ["components/memo/*"],
      "@notifications/*": ["components/notifications/*"],
      "@officeBudget/*": ["components/officeBudget/*"],
      "@paymentVoucher/*": ["components/paymentVoucher/*"],
      "@payroll/*": ["components/payroll/*"],
      "@procurements/*": ["components/procurements/*"],
      "@staff/*": ["components/staff/*"]
    },
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "erasableSyntaxOnly": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true
  },
  "include": ["src"]
}
